<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kakaocorp.iamguide.dao.UploadMapper">
    <insert id = "setImage">
        insert into GUIDE_IMAGE(IMAGE_PATH)
        values(#{url})
    </insert>

    <select id="insertTag">
        ${insertTag}
    </select>
    <select id="insertTagging">
        ${insertTagging}
    </select>
    <select id="deleteTag">
        ${deleteTag}
    </select>
</mapper>